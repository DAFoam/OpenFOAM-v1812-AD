// This dictionary only works in combination with constant/coordinateSystems

mycs1
{
    type   indirect;
    name   cs1;
}

mycs2
{
    type   indirect;
    name   cs2;
}

mycs3
{
    type   indirect;
    name   cs3;
}

mycyl
{
    type   indirect;
    name   cyl;
}


mycy2
{
    coordinateSystem
    {
        type   indirect;
        name   cyl;
    }
}

mycy3
{
    coordinateSystem cyl;
}


