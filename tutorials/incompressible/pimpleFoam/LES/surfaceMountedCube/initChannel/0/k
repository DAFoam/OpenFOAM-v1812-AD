FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 18.06; // 0.1;

boundaryField
{
    lowerWall
    {
        type            fixedValue;
        value           uniform 1e-10;
    }
    upperWall
    {
        type            fixedValue;
        value           uniform 1e-10;
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    frontAndBack
    {
        type            empty;
    }
}


