FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      hTotal;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    sides
    {
        type            calculated;
        value           uniform 0.01;
    }
    inlet
    {
        type            calculated;
        value           uniform 0.01;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.01;
    }
    frontAndBack
    {
        type            empty;
    }
}


