FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [1 -1 -2 0 0 0 0];

internalField       uniform 0;

boundaryField
{
    topSurface
    {
        type                calculated;
        value               uniform 0;
    }

    bottomSurface
    {
        type                calculated;
        value               uniform 0;
    }

    fixedEnd
    {
        type                calculated;
        value               uniform 0;
    }

    tractionEnd
    {
        type                calculated;
        value               uniform 0;
    }

    defaultFaces
    {
        type                empty;
    }
}

