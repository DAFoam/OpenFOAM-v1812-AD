FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    outlet
    {
        type        fixedFluxPressure;
    }

    ground
    {
        type        fixedFluxPressure;
    }

    side
    {
        type            totalPressure;
        p0              $internalField;
    }

    frontAndBack
    {
        type            empty;
    }

    region0_to_panelRegion_panel
    {
        type            fixedFluxPressure;
    }
}


// ************************************************************************* //
