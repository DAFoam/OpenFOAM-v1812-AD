    (-15 $r3CosTO $r3SinTO)
    (-15 $r3CosU  $r3SinU )

    (  0 $r2CosT  $r2SinT )
    (  0 $r2CosTO $r2SinTO)
    (  0 $r2CosU  $r2SinU )

    ( 12 $r2CosT  $r2SinT )
    ( 12 $r2CosTO $r2SinTO)
    ( 12 $r2CosU  $r2SinU )
    ( 12 $r2CosUO $r2SinUO)

