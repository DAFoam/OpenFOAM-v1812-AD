FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 100;

boundaryField
{
    "(walls|cylinder)"
    {
        type            epsilonWallFunction;
        value           uniform 100;
    }
    "inlet.*"
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.001;
        value           uniform 100;
    }
    outlet
    {
        type           inletOutlet;
        inletValue      uniform 100;
        value           uniform 100;
    }
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
