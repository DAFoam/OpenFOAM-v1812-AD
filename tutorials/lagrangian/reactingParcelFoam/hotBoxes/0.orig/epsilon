FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-7;

boundaryField
{
    walls
    {
        type            epsilonWallFunction;
        value           uniform 1e-7;
    }
    wallFilm
    {
        type            epsilonWallFunction;
        value           uniform 1e-7;
    }
}


