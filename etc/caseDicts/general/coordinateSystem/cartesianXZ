FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      cartesianXZ;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type      cartesian;

origin    (0 0 0);

rotation
{
    type  axes;
    e1    $x;
    e3    $z;
}

